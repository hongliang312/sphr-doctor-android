<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.lightheart.sphr.doctor.module.home.activity.TestDetailsActivity">

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fitsSystemWindows="true"
        android:theme="@style/AppTheme.AppBarOverlay"
        app:elevation="@dimen/dp_0">

    <include layout="@layout/common_toolbar"/>

    </android.support.design.widget.AppBarLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <RelativeLayout
                android:id="@+id/rela"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                >

                <TextView
                    android:id="@+id/title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_14"
                    android:textSize="@dimen/medium"
                    android:layout_marginTop="@dimen/dp_5"
                    />

                <TextView
                    android:id="@+id/tiemm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/testingtime"
                    android:layout_marginStart="@dimen/dp_14"
                    android:layout_below="@+id/title"
                    android:layout_marginTop="@dimen/dp_14"
                    android:textColor="@color/colorDayTextLight"
                    android:textSize="@dimen/big_small"
                    />

                <TextView
                    android:id="@+id/time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_2017_12_03_2018_01_29"
                    android:layout_marginStart="@dimen/dp_14"
                    android:layout_below="@+id/title"
                    android:layout_toEndOf="@+id/tiemm"
                    android:layout_marginTop="@dimen/dp_14"
                    android:textColor="@color/sky_blue"
                    android:textSize="@dimen/big_small"
                    />

                <TextView
                    android:id="@+id/numberr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/recruit"
                    android:layout_marginStart="@dimen/dp_14"
                    android:layout_below="@+id/time"
                    android:layout_marginTop="@dimen/dp_14"
                    android:textColor="@color/colorDayTextLight"
                    android:textSize="@dimen/big_small"
                    />

                <TextView
                    android:id="@+id/numb"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_14"
                    android:layout_below="@+id/time"
                    android:layout_toEndOf="@+id/numberr"
                    android:layout_marginTop="@dimen/dp_14"
                    android:textColor="@color/sky_blue"
                    android:textSize="@dimen/big_small"
                    android:text="@string/man"
                    />

                <TextView
                    android:id="@+id/indicationn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/adaptationdisease"
                    android:layout_marginStart="@dimen/dp_14"
                    android:layout_below="@+id/numb"
                    android:layout_marginTop="@dimen/dp_14"
                    android:textColor="@color/colorDayTextLight"
                    android:textSize="@dimen/big_small"
                    />

                <TextView
                    android:id="@+id/Indication"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_14"
                    android:layout_below="@+id/numb"
                    android:layout_toEndOf="@+id/indicationn"
                    android:layout_marginTop="@dimen/dp_14"
                    android:textColor="@color/sky_blue"
                    android:textSize="@dimen/big_small"
                    />
            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_6"
                android:layout_marginTop="@dimen/dp_24"
                android:background="@color/colorDayTextLight"
                />



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/purposeofresearch"
                    android:layout_marginStart="@dimen/dp_14"
                    android:layout_marginTop="@dimen/dp_30"
                    android:textColor="@color/colorDayTextLight"
                    android:textSize="@dimen/medium"
                    />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginTop="@dimen/dp_6"
                    android:background="@color/colorDayTextLight"
                    />

                <TextView
                    android:id="@+id/COronary"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_14"
                    android:layout_marginTop="@dimen/dp_4"
                    android:textSize="@dimen/medium"
                    android:textColor="@color/title_black"
                    />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginTop="@dimen/dp_4"
                    android:background="@color/colorDayTextLight"
                    />

            </LinearLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_14"
                >

                <TextView
                    android:id="@+id/ProjectSponsorr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/projectsponsor"
                    android:layout_marginStart="@dimen/dp_14"
                    android:textColor="@color/colorDayTextLight"
                    android:layout_marginTop="@dimen/dp_10"
                    android:textSize="@dimen/big_small"
                    />

                <TextView
                    android:id="@+id/ProJectSponsor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@+id/ProjectSponsorr"
                    android:textColor="@color/sky_blue"
                    android:layout_marginTop="@dimen/dp_10"
                    android:textSize="@dimen/big_small"
                    android:layout_marginStart="@dimen/dp_15"
                    />

                <TextView
                    android:id="@+id/Projectorganizerr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/projectorganizer"
                    android:layout_marginStart="@dimen/dp_14"
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_below="@+id/ProjectSponsorr"
                    android:textColor="@color/colorDayTextLight"
                    android:textSize="@dimen/big_small"
                    />

                <TextView
                    android:id="@+id/Projectorganizer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@+id/ProjectSponsorr"
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_below="@+id/ProjectSponsorr"
                    android:textColor="@color/sky_blue"
                    android:textSize="@dimen/big_small"
                    android:layout_marginStart="@dimen/dp_15"
                    />

                <TextView
                    android:id="@+id/projectPii"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/projectpi"
                    android:layout_marginStart="@dimen/dp_14"
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_below="@+id/Projectorganizerr"
                    android:textColor="@color/colorDayTextLight"
                    android:textSize="@dimen/big_small"
                    />

                <TextView
                    android:id="@+id/Projecttestphasee"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/projectPii"
                    android:layout_alignBottom="@+id/projectPii"
                    android:layout_toEndOf="@+id/Projectorganizerr"
                    android:layout_marginTop="@dimen/dp_20"
                    android:textSize="@dimen/big_small"
                    />


                <TextView
                    android:id="@+id/Projecttestphase"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/projectphase"
                    android:layout_marginStart="@dimen/dp_14"
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_below="@+id/projectPii"
                    android:textColor="@color/colorDayTextLight"
                    android:textSize="@dimen/big_small"
                    />

                <TextView
                    android:id="@+id/ProJectPI"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@+id/Projecttestphase"
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_below="@+id/projectPii"
                    android:textColor="@color/sky_blue"
                    android:textSize="@dimen/big_small"
                    android:layout_marginStart="@dimen/dp_15"
                    />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_6"
                android:layout_marginTop="@dimen/dp_25"
                android:background="@color/colorDayTextLight"
                />


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_30"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/message"
                    android:layout_marginStart="@dimen/dp_10"
                    android:textColor="@color/colorPrimaryDayAccent"
                    android:textSize="@dimen/big_small"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/r1"
                android:background="#00669A"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginLeft="@dimen/dp_6"
                android:layout_marginRight="@dimen/dp_6"
                android:layout_marginTop="@dimen/dp_14"
                >
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_weight="1"
                        android:textColor="#fff"
                        android:gravity="center"
                        android:text="@string/projectlocation"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#fff"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:textColor="#fff"
                        android:gravity="center"
                        android:text="@string/researcher"
                        android:layout_weight="2"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#fff"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:textColor="#fff"
                        android:gravity="center"
                        android:text="@string/plan"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#fff"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:textColor="#fff"
                        android:gravity="center"
                        android:text="@string/thegroupnumber"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#fff"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:textColor="#fff"
                        android:gravity="center"
                        android:text="@string/thegrouprate"
                        android:layout_weight="1.5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#fff"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:textColor="#fff"
                        android:gravity="center"
                        android:text="@string/state"
                        android:layout_weight="1.5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#fff"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:textColor="#fff"
                        android:gravity="center"
                        android:text="@string/isstart"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#fff"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:textColor="#fff"
                        android:gravity="center"
                        android:text="@string/earlyexit"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/r2"
                android:layout_width="match_parent"
                android:layout_height="130dp"
                android:layout_marginLeft="@dimen/dp_6"
                android:layout_marginRight="@dimen/dp_6"
                >
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:id="@+id/siteName"
                        android:layout_weight="1"
                        android:textColor="#000"
                        android:gravity="center"

                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>


                    <TextView
                        android:id="@+id/contacts"
                        android:textColor="#000"
                        android:gravity="center"

                        android:layout_weight="2"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />


                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:id="@+id/plannedNum"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:id="@+id/involvedNum"
                        android:textColor="#000"
                        android:gravity="center"

                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:id="@+id/Thegrouprate"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="1.5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:id="@+id/State"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="1.5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:id="@+id/isStart"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:id="@+id/exitedNum"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                </LinearLayout>
            </RelativeLayout>
            <View
                android:id="@+id/v1"
                android:background="#000"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"/>

            <View
                android:id="@+id/v2"
                android:background="#000"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"/>
            <RelativeLayout
                android:id="@+id/r4"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginLeft="@dimen/dp_6"
                android:layout_marginRight="@dimen/dp_6"
                >
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    >
                    <TextView
                        android:id="@+id/Total"
                        android:layout_weight="1"
                        android:textColor="#000"
                        android:gravity="center"
                        android:text="@string/total"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:id="@+id/Totalcontacts"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="2"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:id="@+id/TotalplannedNum"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:id="@+id/TotalinvolvedNum"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:id="@+id/TotalThegrouprate"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="1.5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="1.5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                    <View
                        android:background="#000"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:id="@+id/TotalexitedNum"
                        android:textColor="#000"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent" />
                </LinearLayout>
            </RelativeLayout>

            <View
                android:background="#000"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"/>

        </LinearLayout>



    </ScrollView>

</LinearLayout>
